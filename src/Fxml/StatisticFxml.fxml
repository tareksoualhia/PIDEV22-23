<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Fxml.StatisticFxmlController">
   <children>
      <VBox alignment="CENTER" prefHeight="394.0" prefWidth="586.0" AnchorPane.leftAnchor="0.0">
         <children>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="400.0" prefWidth="600.0">
               <children>
                  <BarChart fx:id="barChart" legendVisible="false" prefHeight="381.0" prefWidth="512.0" stylesheets="@style.css" title="Formation by Category">
                    <xAxis>
                      <CategoryAxis label="Category" side="BOTTOM" fx:id="xAxis" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis fx:id="yAxis" label="Nombre De Formation" side="LEFT" />
                    </yAxis>
                  </BarChart>
               </children>
            </HBox>
            <Button fx:id="btnPrint" mnemonicParsing="false" onAction="#print" prefHeight="22.0" prefWidth="218.0" styleClass="btn" stylesheets="@style.css" text="Print">
               <VBox.margin>
                  <Insets left="90.0" />
               </VBox.margin></Button>
         </children>
         <padding>
            <Insets left="90.0" top="50.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
