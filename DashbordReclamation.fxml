<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="510.0" prefWidth="792.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.DashbordReclamationController">
   <children>
      <TableView fx:id="tabrec" layoutX="93.0" layoutY="87.0" prefHeight="246.0" prefWidth="422.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="53.0" text="ID" />
          <TableColumn fx:id="type" prefWidth="191.0" text="Type" />
            <TableColumn fx:id="description" prefWidth="97.0" text="Description" />
            <TableColumn fx:id="date" text="Date" />
        </columns>
     
      </TableView>
      <Button fx:id="refresh" layoutX="96.0" layoutY="408.0" mnemonicParsing="false" onAction="#Actualiser" text="refresh" />
      <Button fx:id="delete" layoutX="255.0" layoutY="408.0" mnemonicParsing="false" onAction="#Supprimer" text="delete" />
      <Button fx:id="cancel" layoutX="424.0" layoutY="401.0" mnemonicParsing="false" onAction="#retour" text="Return" />
      <Label layoutX="269.0" layoutY="48.0" text="liste reclamations" />
      <Button fx:id="update" layoutX="575.0" layoutY="408.0" mnemonicParsing="false" onAction="#modifier" text="modifier" />
      <Button fx:id="repondre" layoutX="651.0" layoutY="318.0" mnemonicParsing="false" onAction="#repondre" text="repondre" textAlignment="CENTER" />
      <ComboBox fx:id="Trie" layoutX="576.0" layoutY="69.0" onAction="#Trie" prefWidth="150.0" promptText="E/C/Ev" />
   </children>
</AnchorPane>
